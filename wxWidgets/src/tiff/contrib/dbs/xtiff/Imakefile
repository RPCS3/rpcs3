   SYS_LIBRARIES = -lm
 LOCAL_LIBRARIES = XawClientLibs
         DEPLIBS = XawClientDepLibs
            TIFF = ../../../libtiff
 EXTRA_LIBRARIES = $(TIFF)/libtiff.so -lm
  EXTRA_INCLUDES = -I$(TIFF)

SimpleProgramTarget(xtiff)
